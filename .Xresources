xterm*SaveLines: 999
xterm*VT100*Translations: #override \
        Ctrl Shift <KeyPress> V: insert-selection(CLIPBOARD, CUT_BUFFER1) \n\
        Ctrl <KeyPress> V: insert-selection(PRIMARY, CUT_BUFFER0) \n\
        <BtnUp> : select-end(CLIPBOARD, CUT_BUFFER1) \n\
        <BtnUp> : select-end(PRIMARY, CUT_BUFFER0) \n
