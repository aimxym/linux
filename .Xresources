xterm*termName: xterm-256color
xterm*locale: true
xterm*utf8: true
xterm*utf8Title: true
xterm*fontMenu*fontdefault*Label: Default
xterm*faceName: Monospace
xterm*faceNameDoublesize: WenQuanYi Micro Hei
xterm*faceSize: 11
